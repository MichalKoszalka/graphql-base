type Mutation {
	createUser(name: String!, authProvider: AuthData!): User
	createLink(url: String!, description: String!): Link
	singinUser(auth: AuthData): SigninPayload
}

type SigninPayload {
	token: String
	user: User
}

type Link {
	id: ID!
	url: String!
	description: String!
}

type User {
	id: ID!
	name: String!
	email: String!
	password: String!
}

input AuthData {
	email: String!
	password: String!
}

type Query {
	allLinks: [Link]
}

schema {
	query: Query
	mutation: Mutation
}
